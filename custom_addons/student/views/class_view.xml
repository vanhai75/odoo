<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record id="school_school_information_form_view" model="ir.ui.view">
            <field name="name">class.class.information.form.view</field>
            <field name="model">class.information</field>
            <field name="inherit_id" ref="class.class_information_form_view"/>
            <field name="arch" type="xml">
                <xpath expr="//group[@name='view_student']" position="inside">
                    <field name="student_list">
                        <tree>
                                 <field name="name" />
                                <field name="birthday"/>

                        </tree>
                    </field>
                </xpath>
            </field>
        </record>
    </data>
</odoo>